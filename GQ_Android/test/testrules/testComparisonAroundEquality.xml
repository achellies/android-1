<?xml version="1.0" encoding="UTF-8"?>

<test>
	<rule>
		<action type="SetVariable" var="a">
			<value>
				<num>1</num>
			</value>
		</action>
	</rule>

	<rule>
		<if>
			<leq>
				<var>a</var>
				<num>1</num>
			</leq>
		</if>
		<action type="IncrementVariable" var="a" />
	</rule>

	<rule>
		<if>
			<geq>
				<var>a</var>
				<num>2</num>
			</geq>
		</if>
		<action type="IncrementVariable" var="a" />
	</rule>

	<rule>
		<if>
			<eq>
				<var>a</var>
				<num>3</num>
			</eq>
		</if>
		<action type="IncrementVariable" var="a" />
	</rule>
</test>
